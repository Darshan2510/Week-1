#include<iostream>
using namespace std;

int main(){
    int n;
    cin>>n;
    int A[n][3];
    for(int i=0;i<n;i++){
        for(int j=0;j<3;j++){
            cin>>A[i][j];
        }
        cout<<endl;
    }
   
    
    for(int i=0;i<n;i++){
        if(A[i][0]+A[i][1]>=10||A[i][1]+A[i][2]>=10||A[i][2]+A[i][0]>=10) cout<<"YES";
        else cout<<"NO";
        cout<<endl;
    }
    return 0;
}
    
