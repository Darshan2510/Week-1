#include <iostream>
using namespace std;

int main(){
    int t;
    cin>> t;
    int arr[t];
    for(int i=0;i<t;i++){
        cin>>arr[i];
    }
    for(int i=0;i<t;i++){
    int count=0;
        for(int j=1;j<arr[i];j++){
            if(arr[i]%j!=0) count+=1;

        }
            cout<<count+1;
            cout<<endl;
            
    }

    return 0;}
